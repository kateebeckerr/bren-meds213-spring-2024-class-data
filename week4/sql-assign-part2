-- 2. Who worked with who?  

SELECT * FROM Camp_assignment;

SELECT A.Site,
       A.Observer AS Observer_1,
       B.Observer AS Observer_2
FROM Camp_assignment A
JOIN Camp_assignment B ON A.Site = B.Site
                        AND A.Start <= B.End
                        AND A.End >= B.Start
                        AND A.Observer < B.Observer
WHERE A.Site = 'lkri';
